---
import type { Website } from "@/components/pages/Website/website.types";
import Hero4 from "@/components/websites/components/Hero4.astro";
import Gallery1 from "../components/Gallery1.astro";
import Ubication1 from "../components/Ubication1.astro";
import About1 from "../components/About1.astro";
import Mission1 from "../components/Mission1.astro";

interface Props {
    webpage: Website;
}

const { webpage } = Astro.props;
---

<Hero4 webpage={webpage} />
<About1 language={webpage.language} about={webpage.about} pastors={webpage.pastors} pastorsImg={webpage.pastorsImg} animations={webpage.animations} />
<Mission1 mission={webpage.mission} animations={webpage.animations} />
<Gallery1 language={webpage.language} images={webpage.images} animations={webpage.animations} />
<Ubication1 language={webpage.language} mapsLink={webpage.mapsLink} ubication={webpage.ubication} animations={webpage.animations} />
